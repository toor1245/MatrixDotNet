<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>default</LangVersion>
        <PackageVersion>0.0.3.1</PackageVersion>
        <Title>MatrixDotNet</Title>
        <Authors>Nikolay Hohsadze</Authors>
        <PackageProjectUrl>https://toor1245.github.io/MatrixDotNet/</PackageProjectUrl>
        <PackageIconUrl></PackageIconUrl>
        <RepositoryUrl>https://github.com/toor1245/MatrixDotNet</RepositoryUrl>
        <PackageTags>matrix,Matrix,MatrixDotNet,Math,linear algebra,statistics,solve,factorization,decomposition,dotnet,AVX,Sse,SIMD,optimization</PackageTags>
        <NeutralLanguage>en</NeutralLanguage>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageIcon>MatrixDotNet_128x128.png</PackageIcon>
        <Version>0.0.3.1</Version>
        <DocumentationMarkdownURL>https://github.com/toor1245/MatrixDotNet/blob/master/README.md</DocumentationMarkdownURL>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <Description>Powerful MatrixDotNet library for matrix calculation</Description>
        <RepositoryType>Library</RepositoryType>
        <PackageReleaseNotes>Fixed Multipy Strassen.
Add Lup factorization and determinant.
Stable works with MatrixDotNet.Extensions.Core.</PackageReleaseNotes>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <Copyright>MatrixDotNet</Copyright>
        <Company>MatrixDotNet</Company>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <ApplicationIcon>MatrixDotNet.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\..\LICENSE">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
      <None Include="images\MatrixDotNet_128x128.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

</Project>
