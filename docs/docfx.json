{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "src/MatrixDotNet*/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "template-theme"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata" :
    {
      "_appTitle" : "MatrixDotNet",
      "_appFooter" : "Copyright &copy; 2020-2021 MatrixDotNet",
      "_appLogoPath" : "images/logo/matrix.svg",
      "_appFaviconPath": "images/logo/matrix.ico",
      "_enableSearch": true
    },
    "sitemap":
    {
      "baseUrl": " https://toor1245.github.io/MatrixDotNet/"
    }
  }
}